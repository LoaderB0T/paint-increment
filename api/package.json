{
  "name": "paint-increment-api",
  "private": true,
  "version": "1.0.0",
  "description": "Paint a picture incrementally with your friends",
  "license": "MIT",
  "author": {
    "name": "Janik Schumacher (LoaderB0T)",
    "url": "https://awdware.de"
  },
  "homepage": "https://github.com/LoaderB0T/paint-increment",
  "repository": {
    "type": "git",
    "url": "https://github.com/LoaderB0T/paint-increment.git"
  },
  "prettier": "@awdware/prettier-config",
  "scripts": {
    "link-file": "pnpm ts-node ./link-events.ts",
    "prebuild": "rimraf dist",
    "build": "nest build && pnpm copy:assets",
    "copy:assets": "cpy config.json dist && cpy web.config dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "pnpm migrate:up",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:base": "pnpm ts-node node_modules/migrate-mongo/bin/migrate-mongo.js",
    "migrate:status": "pnpm migrate:base status -f migrate-mongo-config.ts",
    "migrate:create": "pnpm migrate:base create -f migrate-mongo-config.ts",
    "migrate:up": "pnpm migrate:base up -f migrate-mongo-config.ts",
    "migrate:undo-last": "pnpm migrate:base down -f migrate-mongo-config.ts && pnpm migrate:status"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.16",
    "@nestjs/core": "^10.1.2",
    "@nestjs/platform-express": "^10.1.2",
    "@nestjs/swagger": "^7.1.4",
    "canvas": "^3.2.1",
    "mongodb": "^5.8.0",
    "nanoid": "^5.0.9",
    "nodemailer": "^7.0.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1",
    "supertokens-node": "^15.0.0"
  },
  "devDependencies": {
    "@awdware/eslint-config-base": "^1.1.0",
    "@awdware/eslint-config-nodejs": "^1.0.0",
    "@awdware/prettier-config": "^1.0.0",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.22.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.6.2",
    "migrate-mongo": "^10.0.0",
    "npm-check-updates": "^16.10.17",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
